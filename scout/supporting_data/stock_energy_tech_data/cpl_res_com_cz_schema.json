{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://scout.energy.gov/schemas/cpl/v1.0.0",
    "version": "1.0.0",
    "title": "Cost Performance Lifetime Data by Climate Zone",
    "description": "Schema for validating equipment cost, performance, and lifetime (CPL) data for residential and commercial buildings by AIA climate zone. This schema defines cost structures (including incentives), performance metrics, lifetime data, and consumer choice parameters.",
    "type": "object",
    "patternProperties": {
        "^AIA_CZ[1-5]$": {
        "type": "object",
        "title": "AIA Climate Zone",
        "description": "American Institute of Architects (AIA) Climate Zones 1-5 representing different climatic regions (CZ1=Hot-Humid, CZ2=Hot-Dry, CZ3=Warm, CZ4=Mixed, CZ5=Cold)",
        "properties": {
            "single family home": { "$ref": "#/definitions/buildingTypeRes" },
            "multi family home": { "$ref": "#/definitions/buildingTypeRes" },
            "mobile home": { "$ref": "#/definitions/buildingTypeRes" },
            "assembly": { "$ref": "#/definitions/buildingTypeCom" },
            "education": { "$ref": "#/definitions/buildingTypeCom" },
            "food sales": { "$ref": "#/definitions/buildingTypeCom" },
            "food service": { "$ref": "#/definitions/buildingTypeCom" },
            "health care": { "$ref": "#/definitions/buildingTypeCom" },
            "lodging": { "$ref": "#/definitions/buildingTypeCom" },
            "large office": { "$ref": "#/definitions/buildingTypeCom" },
            "small office": { "$ref": "#/definitions/buildingTypeCom" },
            "mercantile/service": { "$ref": "#/definitions/buildingTypeCom" },
            "warehouse": { "$ref": "#/definitions/buildingTypeCom" },
            "other": { "$ref": "#/definitions/buildingTypeCom" },
            "unspecified": { "$ref": "#/definitions/buildingTypeCom" }
        },
        "additionalProperties": false
        }
    },
    "definitions": {
