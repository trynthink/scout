            "description": "Year-keyed numeric data (2020-2050) or single constant value",
            "anyOf": [
            {
                "type": "object",
                "description": "Object with year keys mapping to numeric values",
                "patternProperties": {
                "^(201[0-9]|202[0-9]|203[0-9]|204[0-9]|2050)$": {
                    "type": "number"
                }
                },
                "additionalProperties": false
            },
            {
                "type": "number",
                "description": "Single numeric value constant across all years",
                "minimum": 0
            }
            ],
