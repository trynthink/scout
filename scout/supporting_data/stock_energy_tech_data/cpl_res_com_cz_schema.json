                "total homes": {
                    "title": "Total Homes",
                    "description": "Total number of residential homes by year",
                    "$ref": "#/definitions/yearNumberObject"
                },
                "new homes": {
                    "title": "New Homes",
                    "description": "Number of new homes constructed by year",
                    "$ref": "#/definitions/yearNumberObject"
                },
                "total square footage": {
                    "title": "Total Square Footage",
                    "description": "Total residential floor space (square feet) by year",
                    "$ref": "#/definitions/yearNumberObject"
                },
                "electricity": {
                    "type": "object",
                    "patternProperties": {
                        "^heating$": {
                            "type": "object",
                            "properties": {
                                "demand": { "$ref": "#/definitions/demandResObject" },
                                "supply": {
                                    "type": "object",
                                    "patternProperties": {
                                        "resistance heat": {"$ref": "#/definitions/cplObject"},
                                        "ASHP": {"$ref": "#/definitions/cplObject"},
                                        "GSHP": {"$ref": "#/definitions/cplObject"}
                                    },
                                    "additionalProperties": false
                                }
                            },
                            "additionalProperties": false
                        },
                        "^secondary heating$": {
                            "type": "object",
                            "properties": {
                                "demand": { "$ref": "#/definitions/demandResObject" },
                                "supply": {
                                    "type": "object",
                                    "patternProperties": {
                                        "secondary heater": {
                                            "type": "number",
                                            "const": 0,
                                            "description": "Secondary heating not modeled separately in CPL data"
                                        }
                                    },
                                    "additionalProperties": false
                                }
                            },
                            "additionalProperties": false
                        },
                        "cooling": {
                            "type": "object",
                            "properties": {
