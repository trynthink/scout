    "$id": "https://scout.energy.gov/schemas/mseg/v1.0.0",
    "version": "1.0.0",
    "title": "Microsegment Stock Energy Technology Data",
    "description": "Schema for validating microsegment (residential and commercial) stock energy and technology data by climate zone. This schema defines the structure for building stock, energy consumption, and equipment data across AIA climate zones.",
    "type": "object",
    "patternProperties": {
        "^AIA_CZ[1-5]$": {
        "type": "object",
        "title": "AIA Climate Zone",
        "description": "American Institute of Architects (AIA) Climate Zones 1-5 representing different climatic regions (CZ1=Hot-Humid, CZ2=Hot-Dry, CZ3=Warm, CZ4=Mixed, CZ5=Cold)",
