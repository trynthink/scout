            "single family home": { "$ref": "#/definitions/buildingTypeRes" },
            "multi family home": { "$ref": "#/definitions/buildingTypeRes" },
            "mobile home": { "$ref": "#/definitions/buildingTypeRes" },
            "assembly": { "$ref": "#/definitions/buildingTypeCom" },
            "education": { "$ref": "#/definitions/buildingTypeCom" },
            "food sales": { "$ref": "#/definitions/buildingTypeCom" },
            "food service": { "$ref": "#/definitions/buildingTypeCom" },
            "health care": { "$ref": "#/definitions/buildingTypeCom" },
